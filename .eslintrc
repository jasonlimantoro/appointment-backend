{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "plugins": ["prettier"],
  "rules": {
    "max-len": 0,
    "no-use-before-define": 0,
    "no-tabs": 0,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "import/prefer-default-export": 0,
    "arrow-parens": 0
  },
  "env": {
    "jest": true,
    "browser": true
  }
}
