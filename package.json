{
  "name": "appointment_backend",
  "version": "1.1.0",
  "description": "A Node.js starter for the Serverless Framework with async/await and unit test support",
  "main": "handler.js",
  "scripts": {
    "precommit": "lint-staged",
    "pretest": "NODE_ENV=test npx sequelize-cli db:migrate",
    "test": "jest --no-cache",
    "test:watch": "yarn test --watch",
    "preoffline": "NODE_ENV=local npx sequelize-cli db:create",
    "offline": "webpack --config webpack.seed.js && sls offline start --dontPrintOutput",
    "offline:no-cache": "webpack --config webpack.seed.js && sls offline start --dontPrintOutput --skipCacheInvalidation",
    "deploy": "sls deploy -v"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-nodejs-starter.git"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.5.5",
    "@types/jest": "^24.0.15",
    "apollo-server-testing": "^2.6.2",
    "aws-sdk-mock": "^4.5.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import-graphql": "^2.7.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "faker": "^4.1.0",
    "husky": "^3.0.2",
    "jest": "^24.8.0",
    "jest-chain": "^1.1.2",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "serverless-offline": "^3.25.6",
    "serverless-webpack": "^5.1.0",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.3.5",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "@aws-amplify/auth": "^1.2.25",
    "@aws-amplify/core": "^1.0.28",
    "@babel/runtime": "^7.4.5",
    "amazon-cognito-identity-js": "^3.0.13",
    "apollo-errors": "^1.9.0",
    "apollo-server-lambda": "^2.6.4",
    "appointment-common": "^1.0.1",
    "aws-sdk": "^2.473.0",
    "babel-runtime": "^6.26.0",
    "es6-promise": "^4.2.8",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.4",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "merge-graphql-schemas": "^1.5.8",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "node-jose": "^1.1.3",
    "sequelize": "^5.10.1",
    "sequelize-cli": "^5.5.0",
    "serverless-dynamodb-local": "^0.2.38",
    "source-map-support": "^0.4.18",
    "uuid": "^3.3.2"
  },
  "lint-staged": {
    "*.{js,jsx,css}": [
      "prettier --write --single-quote",
      "eslint --fix",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ]
  }
}
